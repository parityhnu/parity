<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lufan.parityproject">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:name="com.lufan.parityproject.ParityApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme">
        <activity android:name="com.lufan.parityproject.Activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.lufan.parityproject.Activity.SearchActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.GoodsListActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.GoodsParityDetailActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.FavoriteActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.ModifyActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.LoginActivity">
        </activity>
        <activity android:name="com.lufan.parityproject.Activity.SignUpActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.ForgetPasswordActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.ModifyDetailActivity"></activity>
        <activity android:name="com.lufan.parityproject.Activity.WebViewActivity"></activity>


    </application>

</manifest>